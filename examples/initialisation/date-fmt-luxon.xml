<css lib="datatables searchbuilder datetime"/>

<js lib="jquery"/>
<js lib="//cdnjs.cloudflare.com/ajax/libs/luxon/1.25.0/luxon.min.js"/>

<js lib="datatables searchbuilder datetime"/>
<js>
<![CDATA[

DataTable.datetime('EEE, MMMM d, yyyy');
DataTable.datetime('HH:mm MMM d, yy');

$('#example').DataTable({
	layout: {
		topStart: 'searchBuilder'
	}
});

]]>
</js>

<js-vanilla>
<![CDATA[

DataTable.datetime('EEE, MMMM d, yyyy');
DataTable.datetime('HH:mm MMM d, yy');

new DataTable('#example', {
	layout: {
		topStart: 'searchBuilder'
	}
});

]]>
</js-vanilla>

<title lib="SearchBuilder">Formatted dates with Luxon</title>

